{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "jder/psr",
    "description": "A response builder for PSR",
    "version": "0.1.0",
    "type": "library",
    "keywords": [
        "jder",
        "psr",
        "response",
        "php"
    ],
    "homepage": "https://github.com/jderstd/psr",
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpheus",
            "email": "contact@alphe.us"
        }
    ],
    "support": {
        "issues": "https://github.com/jderstd/psr/issues",
        "source": "https://github.com/jderstd/psr",
        "docs": "https://github.com/jderstd/psr/blob/main/docs/README.md"
    },
    "require": {
        "php": "^8.1.0",
        "psr/http-message": "^2.0.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8.2",
        "nyholm/psr7-server": "^1.1.0",
        "phpunit/phpunit": "^13.0.2",
        "slim/slim": "^4.15.1",
        "carthage-software/mago": "^1.8.0"
    },
    "autoload": {
        "psr-4": {
            "Jder\\Psr\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "carthage-software/mago": true
        }
    }
}
